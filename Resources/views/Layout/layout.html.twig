{% spaceless %}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Satoripop Crop Images Bundle</title>
    {% stylesheets
    "@SatoripopCropImagesBundle/Resources/public/css/bootstrap.min.css"
    "@SatoripopCropImagesBundle/Resources/public/css/ekko-lightbox.min.css"
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
<div class="container">
    <div class="row">{% block body %}{% endblock body %}</div>
</div>
{% javascripts
"@SatoripopCropImagesBundle/Resources/public/js/bootstrap.min.js"
"@SatoripopCropImagesBundle/Resources/public/js/ekko-lightbox.min.js"
output="/bundles/satoripopcropimages/js/bootstrap.min.js" %}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}
</body>
</html>
{% endspaceless %}